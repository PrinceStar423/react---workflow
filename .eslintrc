{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "__DEV__": true
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true
  },
  "rules": {
    "react/jsx-quotes": 0,
    "no-eval": 1,
    "jsx-quotes": [2, "prefer-double"]
  },
  "plugins": [
    "react"
  ]
}
